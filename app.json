{
  "expo": {
    "name": "plantly-fe",
    "slug": "plantly-fe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/plant-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splashcopy.png",
      "resizeMode": "contain",
      "backgroundColor": "#729d84"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-iconcopy.png",
        "backgroundColor": "#729d84"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
